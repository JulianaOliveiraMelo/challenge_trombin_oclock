<%- include('partials/header') %>
<h2>Promo: <%=promo.name%></h2>
<ul class="cards">
    <% for(let student of promoStudents) {%>
        <li class="card">
            <a href="/student/<%=student.id%>">
                <figure>
                    <img src="http://github.com/<%=student.github_username%>.png" alt="<%=student.github_username%>">
                    <figcaption><%=student.first_name%> <%=student.last_name%></figcaption>
                </figure>
            </a>
        </li>
    <% } %>
</ul>

<%- include('partials/footer') %>